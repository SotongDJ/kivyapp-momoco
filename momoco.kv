<Label>
    font_name: 'database/yahei_mono.ttf'
    color: 0, 0, 0, 1

<Button>
    background_color: 1, 1, 1, 1

<ListData>
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            # self here refers to the widget i.e FloatLayout
            pos: self.pos
            size: self.size

    rows: 4

    GridLayout:
        cols: 2

        Label:
            text: root.usnanTitle

        TextInput:
            id: usnan
            multiline: False

    GridLayout:
        cols: 2

        Label:
            text: root.paswdTitle

        TextInput:
            id: paswd
            password: True
            multiline: False

    GridLayout:
        cols: 2

        Button:
            text: root.sumitText
            on_press: root.sumited(usnan.text,paswd.text)

        Button:
            text: root.closeText
            on_press: root.close()

    GridLayout:
        cols: 4
        Label:
            text: "Name:"
        Label:
            text: root.usnanText
        Label:
            text: "Password:"
        Label:
            text: root.paswdText

