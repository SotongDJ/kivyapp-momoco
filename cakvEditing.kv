<Label>
    font_name: 'database/yahei_mono.ttf'
    color: 0, 0, 0, 1

<Button>
    background_color: 1, 1, 1, 1

<EditCard>
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            # self here refers to the widget i.e FloatLayout
            pos: self.pos
            size: self.size

    rows: 3

    GridLayout:
        cols: 4
        size_hint_y: 1
        Label:
            text: root.uuidTitle
        TextInput:
            id: indata
            multiline: False
            write_tab: False
            on_text_validate: root.callout(uuid=indata.text,tar=datapar)
        Button:
            text: root.calloutText
            on_press: root.callout(uuid=indata.text,tar=datapar)
        Button:
            text: root.cleenText
            on_press: root.cleen(tar=datapar)

    GridLayout:
        id: datapar
        cols: 1
        size_hint_y: 12

<DataPart>
    rows: 11

    GridLayout:
        cols:4
        Label:
            text: root.datteTitle
        TextInput:
            id: datte
            text: root.datteText
            multiline: False
            write_tab: False
            on_text_validate: root.stored(tar=datteResut,fro=datte.text,too=root.datteText,tan='datte')
        Button:
            text: root.storeText
            on_press: root.stored(tar=datteResut,fro=datte.text,too=root.datteText,tan='datte')
        Label:
            id: datteResut
            text: root.datteText

